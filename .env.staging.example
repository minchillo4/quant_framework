# .env.example - Copy to .env.dev or .env.prod and fill in values
# DO NOT commit actual .env files with secrets

MNEMO_ENV=dev
AIRFLOW_UID=50000

# Database Configuration
POSTGRES_HOST=timescaledb
POSTGRES_PORT=5432
POSTGRES_DB=mnemo_quant
POSTGRES_USER=postgres
POSTGRES_PASSWORD=<your_postgres_password>
POSTGRES_MULTIPLE_DATABASES=airflow,quant_dev,quant

# Airflow Configuration
AIRFLOW_SECRET_KEY=<generate_random_secret_key>
AIRFLOW_FERNET_KEY=<generate_fernet_key>
AIRFLOW_DB_PASSWORD=<your_airflow_db_password>
AIRFLOW_USER=admin
AIRFLOW_PASSWORD=<your_airflow_ui_password>
AIRFLOW__CORE__DAGS_FOLDER=/opt/airflow/dags
AIRFLOW__CORE__LOAD_EXAMPLES=false

# Grafana Configuration
GRAFANA_PASSWORD=<your_grafana_password>

# pgAdmin Configuration
PGADMIN_EMAIL=<your_email@example.com>
PGADMIN_PASSWORD=<your_pgadmin_password>

# Application Configuration
LOG_LEVEL=DEBUG
QUANT_APP_USER=admin
QUANT_APP_PASSWORD=<your_app_password>
REDIS_URL=redis://redis:6379/0
PYTHONPATH=/app/src:/app

# Coinalyze API Configuration
# Insert comma-separated API keys below
COINALYZE_API_KEYS=<api_key_1>,<api_key_2>

# Database Connections
TIMESCALE_URL=postgresql://${POSTGRES_USER}:${POSTGRES_PASSWORD}@${POSTGRES_HOST}:${POSTGRES_PORT}/${POSTGRES_DB}
AIRFLOW_CONN_TIMESCALEDB_DEFAULT=postgresql://${POSTGRES_USER}:${POSTGRES_PASSWORD}@${POSTGRES_HOST}:${POSTGRES_PORT}/${POSTGRES_DB}
AIRFLOW_DB_CONNECTION=postgresql://airflow:${AIRFLOW_DB_PASSWORD}@${POSTGRES_HOST}:${POSTGRES_PORT}/airflow

# MinIO Configuration
MINIO_ROOT_USER=admin
MINIO_ROOT_PASSWORD=<your_minio_password>
MINIO_BUCKET_NAME=bronze
MINIO_REGION=us-east-1
MINIO_ENDPOINT=http://minio:9000